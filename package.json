{
  "name": "school_management_system-back",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build-server": "babel src --out-dir dist -s",
    "clean": "rm -rf dist && mkdir dist",
    "build": "yarn run clean && node_modules/.bin/sequelize db:migrate && yarn run build-server",
    "serve": "node dist/index.js",
    "start": "NODE_ENV=production yarn run build && yarn run serve",
    "dev": "NODE_ENV=development  DEBUG=log  source .env && nodemon --exec babel-node src/index.js",
    "migrate": "source .env && node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "bugsnag": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.2.2",
    "sequelize-cli": "^5.4.0"
  }
}
